        <record model="ir.ui.view" id="nantian_erp_weekly_reports_form">
            <field name="name">weekly_reports.form</field>
            <field name="model">nantian_erp.weekly_reports</field>
            <field name="arch" type="xml">
                <form>

                    <button name="copy_report" type="object" string="复制此份周报"  class="oe_highlight" />
                    <sheet>
                 <group>
                     <group string="周报时间段">
                         <field name="date_from" string = "From"/>
                         <field name="date_to" string = "To"/>
                     </group>
                     <group>
                         <field name="user_id" string="创建者" readonly="True"/>
                     </group>
                 </group>
                     <notebook>
                    <page string="售前项目" >
                            <field name="pres_sale_ids" context="{'form_view_ref': 'nantian_erp.nantian_erp_pres_sale_hearder_form'}">
                                    <tree string="Registered feedback" edit="true">
                                        <field name="project_name" string="项目名称"/>
                                        <field name="partner" string="客户名称" />
                                        <field name="process_scrib" string="本周主要进展说明"/>
                                        <field name="before_bid_amount" string="投标金额"/>
                                        <field name="bid_commpany" string="中标单位"/>
                                        <field name="pre_bid_date" string="预计投标日期"/>
                                        <field name="competitors" string="竞争对手"/>
                                        <field name="rate_of_success" string="成功率(%)"/>
                                    </tree>
                            </field>
                    </page>
                    <page string="项目收款" >
                             <field name="gathering_ids" >
                                    <tree string="Registered feedback" edit="true">
                                        <field name="contract_id" string="合同"/>
                                        <field name="gather_count" string="预期收款金额"/>
                                        <field name="gather_progress" string="收款工作进度"/>
                                        <field name="gather_reminder" string="特此提醒，开始准备收款!"/>
                                    </tree>
                            </field>
                    </page>
                    <page string="人员调动">
                             <field name="pers_transfer_ids" context="{'form_view_ref': 'nantian_erp.nantian_erp_pers_transfer_form'}">
                                    <tree string="Registered feedback" edit="true">
                                        <field name="employee_id" string="调动人"/>
                                        <field name="res_contract_name" string="合同名称" />
                                        <field name="res_contract_job" string="合同岗位"/>
                                        <field name="sour_team" string="原项目组"/>
                                        <field name="move_reason" string="调动原因"/>
                                        <field name="move_date" string="调动时间"/>
                                        <field name="before_leader" string="调动前负责人"/>
                                        <field name="after_leader" string="调动后负责人" />
                                        <field name="dis_states" string="调整状态"/>

                                    </tree>
                            </field>
                    </page>
                     <page string="人员离职">
                          <field name="demission_ids" context="{'form_view_ref': 'nantian_erp.nantian_erp_demission_form'}">
                                    <tree string="Registered feedback" edit="true">
                                        <field name="employee_id" string="离职申请人"/>
                                        <field name="contract_name" string="合同名称" />
                                        <field name="contract_post" string="合同岗位"/>
                                        <field name="sro_project" string="项目组"/>
                                        <field name="demission_reason" string="离职原因"/>
                                        <field name="demission_date" string="离职时间"/>
                                        <field name="is_recruit" string="是否招聘"/>
                                        <field name="state" string="状态"/>

                                    </tree>
                            </field>
                    </page>
                     <page string="现有招聘缺口" context="{'form_view_ref': 'nantian_erp_recruit_gap_form'}">
                              <field name = "recruit_gap_ids">
                                  <tree>
                                  <field name="job" string="岗位"/>
                                 <field name="count" string="人数"/>
                                 <field name="reason" string="原因"/>
                                  </tree>
                              </field>
                     </page>
                    <page string="项目进度">
                             <field name="project_progress_ids">
                                    <tree string="Registered feedback" edit="true">
                                        <field name="major_change" string="本周是否重大变更"/>
                                        <field name="repeat" string="本周是否重保"/>
                                        <field name="major_fault" string="本周是否重大事故"/>
                                        <field name="maintenance" string="本周是否主机维护"/>
                                        <field name="ver_on_line" string="本周是否版本上线"/>
                                        <field name="equipment_implementation" string="本周是否设备实施"/>
                                        <field name="special" string="本周是否有特殊事项"/>
                                        <field name="possible_risk" string="本周预计可能风险"/>
                                    </tree>
                            </field>
                    </page>
                     <page string="客户动态或人事变动">
                             <field name="customer_adjust_ids" >
                                    <tree string="Registered feedback" editable="top">
                                        <field name="major_adjust" string = "是否" />
                                        <field name="major_adjust_detail" string = "近一月客户重大动态或人事变动:详情叙述" />
                                    </tree>
                            </field>
                    </page>
                     <page string="项目阶段数目统计">
                          <group string = "所处阶段">
                            <field name="lixiang" placeholder="项目数量"/>
                            <field name="todo" placeholder="项目数量"/>
                            <field name="juys" placeholder="项目数量"/>
                            <field name="ywfw" placeholder="项目数量" required = "True"/>
                            <field name="zhongyan" placeholder="项目数量"/>
                            <field name="djx" placeholder="项目数量"/>
                        </group>
                    </page>
                     </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record model="ir.ui.view" id="nantian_erp_weekly_reports_tree">
            <field name="name">nantian_erp.weekly_reports.tree</field>
